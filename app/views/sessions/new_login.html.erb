<div class="welcome-card">
<div class="welcome">
  <h1>Welcome Back!</h1>
</div>
  <%= form_for :session, url: "/login", html: {class: 'form-basic'} do |f| %>
    <div class='form-input'>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>

    <div class='form-input'>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>

    <%= f.submit "Login" %>
  <% end %>
  </div>
